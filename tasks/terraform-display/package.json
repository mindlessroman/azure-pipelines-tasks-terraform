{
  "name": "azure-pipelines-tasks-terraform-display",
  "version": "0.1.0",
  "description": "Azure Pipelines task to display terraform plans",
  "main": ".bin/index.js",
  "scripts": {
    "build": "npx tsc --build",
    "test": "cucumber-js -p default",
    "test:coverage": "nyc -r cobertura -r html \"cucumber-js\" \"-p\" \"default\"",
    "test:report": "mkdir -p .tests && cucumber-js -p report | grep -Ev '^##vso|^\\[command]|^[a-zA-Z0-9]' | cucumber-junit > ./.tests/results.xml",
    "start": "ts-node --require dotenv/config ./src/index.ts",
    "pack": "npx copyfiles package.json task.json icon.png \".bin/*.js\" .dist && cd .dist && npm install --only=prod",
    "upload": "npx tfx build tasks upload --task-path .dist",
    "delete": "npx tfx build tasks delete --task-id 320ec1e4-3b96-11eb-adc1-0242ac120002"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charleszipp/azure-pipelines-tasks-terraform.git"
  },
  "keywords": [
    "terraform",
    "azure-devops",
    "vsts"
  ],
  "author": "Alex Goncharov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charleszipp/azure-pipelines-tasks-terraform/issues"
  },
  "homepage": "https://github.com/charleszipp/azure-pipelines-tasks-terraform#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.27",
    "@types/q": "^1.5.1",
    "braces": "^2.3.2",
    "copyfiles": "^2.2.0",
    "cpx": "^1.5.0",
    "mocha": "^7.1.2",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^15.0.0",
    "tfx-cli": "^0.8.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "applicationinsights": "^1.4.0",
    "azure-devops-node-api": "^7.0.0",
    "azure-pipelines-task-lib": "^2.7.1",
    "dotenv": "^8.2.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "terraform-core": "file:../terraform-core"
  }
}
